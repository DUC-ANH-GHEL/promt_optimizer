#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/promt_optimizer/node_modules/.pnpm/vitest@3.2.4_@types+debug@4_4643bb287d9b77d59df8dd7cd1fad566/node_modules/vitest/node_modules:/mnt/d/promt_optimizer/node_modules/.pnpm/vitest@3.2.4_@types+debug@4_4643bb287d9b77d59df8dd7cd1fad566/node_modules:/mnt/d/promt_optimizer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/promt_optimizer/node_modules/.pnpm/vitest@3.2.4_@types+debug@4_4643bb287d9b77d59df8dd7cd1fad566/node_modules/vitest/node_modules:/mnt/d/promt_optimizer/node_modules/.pnpm/vitest@3.2.4_@types+debug@4_4643bb287d9b77d59df8dd7cd1fad566/node_modules:/mnt/d/promt_optimizer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
